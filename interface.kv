<BantuanPengguna>:
    name: 'bantuan'
    MDBoxLayout:
        orientation: 'vertical'
        spacing: '15dp'
        MDTopAppBar:
            title: "Bantuan Pengguna"
            md_bg_color: (33/255, 33/255, 33/255, 255)
            left_action_items: [["home", app.goto_main_menu]]
            
        ScrollView:
            MDBoxLayout:
                orientation: 'vertical'
                spacing: '10dp'
                padding: '20dp'
                size_hint_y: None
                height: self.minimum_height
                pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                MDBoxLayout:
                    orientation: 'vertical'
                    spacing: '10dp'
                    padding: '10dp'
                    size_hint_y: None
                    height: self.minimum_height
                    pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                    MDLabel:
                        text: 'Tata Cara Enkripsi Citra:'
                        font_style: "H6"
                        theme_text_color: "Secondary"
                        size_hint_y: None
                        height: self.texture_size[1]
                    MDLabel:
                        text: "1. Buka menu enkripsi pada aplikasi."
                        font_style: "Subtitle2"
                        theme_text_color: "Primary"
                        size_hint_y: None
                        height: self.texture_size[1]
                    MDLabel:
                        text: "Klik tombol 'Pilih Citra' dan pilih citra dengan ekstensi .jpg, .png, atau .bmp."
                        font_style: "Subtitle2"
                        theme_text_color: "Primary"
                        size_hint_y: None
                        height: self.texture_size[1]
                    MDLabel:
                        text: "2. Masukkan kunci rahasia sesuai ketentuan:"
                        font_style: "Subtitle2"
                        theme_text_color: "Primary"
                        size_hint_y: None
                        height: self.texture_size[1]
                    MDLabel:
                        text: "Kunci 'a' (0.1-0.9), kunci 'b' (0.1-0.9)"
                        font_style: "Subtitle2"
                        theme_text_color: "Primary"
                        size_hint_y: None
                        height: self.texture_size[1]
                    MDLabel:
                        text: "3. Klik tombol 'Enkripsi' untuk mulai proses enkripsi."
                        font_style: "Subtitle2"
                        theme_text_color: "Primary"
                        size_hint_y: None
                        height: self.texture_size[1]
                    MDLabel:
                        text: "Jika berhasil, muncul dialog 'Enkripsi berhasil' dan hasil enkripsi ditampilkan pada card di bagian bawah."
                        font_style: "Subtitle2"
                        theme_text_color: "Primary"
                        size_hint_y: None
                        height: self.texture_size[1]
                MDBoxLayout:
                    orientation: 'vertical'
                    spacing: '10dp'
                    padding: '10dp'
                    size_hint_y: None
                    height: self.minimum_height
                    pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                    MDLabel:
                        text: 'Tata Cara Dekripsi Citra:'
                        font_style: "H6"
                        theme_text_color: "Secondary"
                        size_hint_y: None
                        height: self.texture_size[1]
                    MDLabel:
                        text: "1. Buka menu dekripsi pada aplikasi."
                        font_style: "Subtitle2"
                        theme_text_color: "Primary"
                        size_hint_y: None
                        height: self.texture_size[1]
                    MDLabel:
                        text: "Klik tombol 'Pilih Citra' dan pilih citra terenkripsi dengan ekstensi .png."
                        font_style: "Subtitle2"
                        theme_text_color: "Primary"
                        size_hint_y: None
                        height: self.texture_size[1]
                    MDLabel:
                        text: "2. Masukkan kunci-kunci yang digunakan saat proses enkripsi."
                        font_style: "Subtitle2"
                        theme_text_color: "Primary"
                        size_hint_y: None
                        height: self.texture_size[1]
                    MDLabel:
                        text: "3. Klik tombol 'Dekripsi' untuk memulai proses dekripsi citra."
                        font_style: "Subtitle2"
                        theme_text_color: "Primary"
                        size_hint_y: None
                        height: self.texture_size[1]
                    MDLabel:
                        text: "Jika kunci rahasia cocok dan proses dekripsi berhasil, maka hasil dekripsi citra akan ditampilkan dalam bentuk kartu."
                        font_style: "Subtitle2"
                        theme_text_color: "Primary"
                        size_hint_y: None
                        height: self.texture_size[1]
                MDBoxLayout:
                    orientation: 'vertical'
                    spacing: '10dp'
                    padding: '10dp'
                    size_hint_y: None
                    height: self.minimum_height
                    pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                    MDLabel:
                        text: "Pastikan untuk membuka menu enkripsi atau dekripsi pada aplikasi dan ikuti langkah-langkah yang disebutkan di atas."
                        font_style: "Subtitle2"
                        theme_text_color: "Primary"
                        size_hint_y: None
                        height: self.texture_size[1]
                    
<TentangAplikasi>:
