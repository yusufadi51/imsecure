#:kivy 1.1.0

<MenuUtama>:
    name: 'menuutama'
    MDBoxLayout:
        md_bg_color: (0.047, 0.208, 0.416, 1)
        orientation:'vertical'
        spacing: '15dp'
        MDTopAppBar:
            title:"Image Encryption Platform"
            md_bg_color: (33/255,33/255,33/255,255)
        
        ScrollView:
            bar_width:0
            MDBoxLayout:
                orientation:'vertical'
                adaptive_height: True
                spacing:'40dp'
                padding:'20dp'

               
                MDGridLayout:
                    cols:2
                    adaptive_height:True
                    spacing:"15dp"
                    GridCard:
                        source:'images\lock.png'
                        md_bg_color: (37/255, 150/255, 190/255, 255)
                        text:'Enkripsi Gambar'
                        folder_size:' '
                        on_release:
                            root.manager.transition.direction = 'left'
                            root.manager.current = 'enkripsi'
                    GridCard:
                        source:'images\key.png'
                        md_bg_color: (37/255, 150/255, 190/255, 255)
                        text:'Dekripsi Gambar'
                        folder_size:' '
                        on_release:
                            root.manager.transition.direction = 'left'
                            root.manager.current = 'dekripsi'
                    

                    GridCard:
                        source:'images\histogram.png'
                        md_bg_color: (37/255, 150/255, 190/255, 255)
                        text:'Menu Histogram Citra'
                        folder_size:' '
                        on_release:
                            root.manager.transition.direction = 'left'
                            root.manager.current = 'menuhisto'


                    GridCard:
                        source:'images\question.png'
                        md_bg_color: (37/255, 150/255, 190/255, 255)
                        text:'Bantuan Pengguna'
                        folder_size:' '
                        on_release:
                            root.manager.transition.direction = 'left'
                            root.manager.current = 'bantuan'

<MenuHisto>:
    name: 'menuhisto'
    MDBoxLayout:
        md_bg_color: (0.047, 0.208, 0.416, 1)
        orientation:'vertical'
        spacing: '15dp'
        MDTopAppBar:
            title:"Image Encryption Platform"
            md_bg_color: (33/255,33/255,33/255,255)
            left_action_items: [["home", app.goto_main_menu]]
            
        
        ScrollView:
            bar_width:0
            MDBoxLayout:
                orientation:'vertical'
                adaptive_height: True
                spacing:'40dp'
                padding:'20dp'

               
                MDGridLayout:
                    cols:2
                    adaptive_height:True
                    spacing:"15dp"
                    GridCard:
                        source:'images\histogram.png'
                        md_bg_color: (37/255, 150/255, 190/255, 255)
                        text:'Histogram RGB'
                        folder_size:' '
                        on_release:
                            root.manager.transition.direction = 'left'
                            root.manager.current = 'menurgb'
                    GridCard:
                        source:'images\histogram_gray.png'
                        md_bg_color: (37/255, 150/255, 190/255, 255)
                        text:'Histogram Grayscale'
                        folder_size:' '
                        on_release:
                            root.manager.transition.direction = 'left'
                            root.manager.current = 'menuabu'
                    
        
<MenuEnkripsi>:
    name: 'enkripsi'
    MDBoxLayout:
        md_bg_color: (0.047, 0.208, 0.416, 1)
        orientation: 'vertical'
        spacing: '15dp'
        MDTopAppBar:
            title: "Enkripsi Gambar"
            md_bg_color: (33/255, 33/255, 33/255, 255)
            left_action_items: [["home", app.goto_main_menu]]
        ScrollView:
            bar_width: 0
            spacing: '20dp'
            MDBoxLayout:
                orientation: 'vertical'
                padding: '20dp'
                adaptive_height: True
                spacing: '20dp'
                MDLabel:
                    text: 'Citra Asli'
                    font_style: "Subtitle2"
                MDCard:
                    size_hint_y: None
                    height: '250dp'
                    md_bg_color: (37/255, 150/255, 190/255, 255)
                    radius: 25
                    padding: '15dp'
                    spacing: '15dp'
                    orientation: 'vertical'
                    MDBoxLayout:
                        
                        spacing: '7.5dp'
                        size_hint_y: None
                        height: 150
                        Image:
                            id: img
                            source: 'images\gambar_kosong.png'
                    MDBoxLayout:
                        orientation: 'horizontal'
                        spacing: '10dp'
                        size_hint_y: None
                        height: dp(50) 
                        MDLabel:
                            id: selected_path
                            text: ""
                            font_style: 'Caption'
                            pos_hint: {'center_x': .5, 'center_y': .5}
                            adaptive_height: True
                        MDRaisedButton:
                            markup: True
                            size: 75, 50
                            size_hint: None, None # <---
                            pos_hint: {'center_x': .5, 'center_y': .5}
                            text: f"Pilih Citra [font=Icons]{md_icons['image']}[/font]"
                            on_release: 
                                app.file_chooser(btn_id='img')
                # Key Section
                MDLabel:
                    text: 'Kunci'
                    font_style: "Subtitle2"
                MDCard:
                    md_bg_color: (37/255, 150/255, 190/255, 255)
                    size_hint_y: None
                    height: '80dp'
                    radius: 25
                    padding: '15dp'
                    orientation: 'vertical'
                    
                    MDBoxLayout:
                        spacing: '15dp'
                        MDTextField:
                            id: key_a
                            hint_text: "a"
                            mode: "rectangle"
                            required: True
                        MDTextField:
                            id: key_b
                            hint_text: "b"
                            mode: "rectangle"
                            required: True

                MDRaisedButton:
                    markup: True
                    pos_hint: {'right': 1, 'center_y': 0.5}
                    text: f"Enkripsi [font=Icons]{md_icons['lock']}[/font]"
                    on_release:
                        app.btn_enkripsi()

                MDLabel:
                    text: 'Citra Terenkripsi'
                    font_style: "Subtitle2"
                MDCard:
                    md_bg_color: (37/255, 150/255, 190/255, 255)
                    size_hint_y: None
                    height: '250dp'
                    radius: 25
                    padding: '15dp'
                    spacing: '15dp'
                    orientation: 'vertical'

                    MDBoxLayout:
                        spacing: '7.5dp'
                        size_hint_y: None
                        height: 150

                        Image:
                            id: img_encrypted
                            source: 'images\gambar_enkripsi.png'

                    MDBoxLayout:
                        orientation: 'horizontal'
                        spacing: '10dp'
                        size_hint_y: None
                        height: dp(50) 
                        MDLabel:
                            id: result_path
                            text: ''
                            font_style: 'Caption'
                            adaptive_height: True
                        MDLabel:
                            id: process_time
                            text: "Waktu : "
                            font_style: 'Caption'
                            adaptive_height: True
                            halign:'right'




