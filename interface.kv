                    MDLabel:
                        text: "Versi: 1.0.0"
                        font_style: "Subtitle2"
                        theme_text_color: "Primary"
                        size_hint_y: None
                        height: self.texture_size[1]
                MDBoxLayout:
                    orientation: 'vertical'
                    spacing: '10dp'
                    padding: '10dp'
                    size_hint_y: None
                    height: self.minimum_height
                    pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                    MDLabel:
                        text: "Tujuan: Aplikasi ini bertujuan untuk memberikan pengguna kemampuan untuk melakukan enkripsi dan dekripsi citra dengan menggunakan kunci rahasia. Hal ini membantu meningkatkan keamanan citra yang sensitif."
                        font_style: "Subtitle2"
                        theme_text_color: "Primary"
                        size_hint_y: None
                        height: self.texture_size[1]
                MDBoxLayout:
                    orientation: 'vertical'
                    spacing: '10dp'
                    padding: '10dp'
                    size_hint_y: None
                    height: self.minimum_height
                    pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                    MDLabel:
                        text: "Fitur:"
                        font_style: "Subtitle2"
                        theme_text_color: "Primary"
                        size_hint_y: None
                        height: self.texture_size[1]
                    MDLabel:
                        text: "- Enkripsi citra menggunakan algoritma kriptografi yang kuat."
                        font_style: "Body2"
                        theme_text_color: "Primary"
                        size_hint_y: None
                        height: self.texture_size[1]
                    MDLabel:
                        text: "- Dekripsi citra menggunakan kunci yang sesuai dengan proses enkripsi."
                        font_style: "Body2"
                        theme_text_color: "Primary"
                        size_hint_y: None
                        height: self.texture_size[1]
                    MDLabel:
                        text: "- Dukungan untuk citra dengan format .jpg, .png, dan .bmp."
                        font_style: "Body2"
                        theme_text_color: "Primary"
                        size_hint_y: None
                        height: self.texture_size[1]
                MDBoxLayout:
                    orientation: 'vertical'
                    spacing: '10dp'
                    padding: '10dp'
                    size_hint_y: None
                    height: self.minimum_height
                    pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                    MDLabel:
                        text: "Pengembang:"
                        font_style: "Subtitle2"
                        theme_text_color: "Primary"
                        size_hint_y: None
                        height: self.texture_size[1]
                    MDLabel:
                        text: "- Fakhri Rahmat Rizqi"
                        font_style: "Body2"
                        theme_text_color: "Primary"
                        size_hint_y: None
                        height: self.texture_size[1]
                    MDLabel:
                        text: "Kontak: fakhrirahmatr@gmail.com"
                        font_style: "Subtitle2"
                        theme_text_color: "Primary"
                        size_hint_y: None
                        height: self.texture_size[1]

<GridCard@MDCard>:
    ripple_behavior:True
    orientation:'vertical'
    source:''
    radius:25
    folder_size:''
    text:''
    padding:'20dp'
    spacing:'20dp'
    size_hint_y:None
    height:'150dp'
    Image:
        source:root.source
    MDBoxLayout:
        orientation:'vertical'
        adaptive_height:True
        MDLabel:
            text:root.folder_size
            adaptive_height:True
            halign:'center'
            font_style:'Caption'
        MDLabel:
            text:root.text
            adaptive_height:True
            halign:'center'


